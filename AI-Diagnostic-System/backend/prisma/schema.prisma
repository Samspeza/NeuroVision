generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AnaliseIris {
  id             Int       @id @default(autoincrement())
  nomeArquivo    String
  caminhoImagem  String
  resultado      String
  probabilidade  Float
  distribuicao   Json
  criadoEm       DateTime  @default(now())
  atualizadoEm   DateTime  @updatedAt
  usuario        Usuario?  @relation(fields: [usuarioId], references: [id])
  usuarioId      Int?
}

model Usuario {
  id          Int           @id @default(autoincrement())
  nome        String
  email       String        @unique
  senhaHash   String
  criadoEm    DateTime      @default(now())
  analises    AnaliseIris[]
}
